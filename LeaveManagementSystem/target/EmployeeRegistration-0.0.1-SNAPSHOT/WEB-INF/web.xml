<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:web="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_5_0.xsd" id="WebApp_ID" version="5.0">
  <servlet>
    <servlet-name>bike</servlet-name>
    <servlet-class>Servlet.RegistrationServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>car</servlet-name>
    <servlet-class>Servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>bike</servlet-name>
    <url-pattern>/Registration</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>car</servlet-name>
    <url-pattern>/LoginUser</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>Login.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>Employee</servlet-name>
    <servlet-class>Servlet.EmployeeDetailsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Employee</servlet-name>
    <url-pattern>/EmployeeDetails</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>LeaveBalance</servlet-name>
    <servlet-class>Servlet.LeaveBalanceServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LeaveBalance</servlet-name>
    <url-pattern>/LeaveBalance</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>LeaveDetailServlet</display-name>
    <servlet-name>LeaveDetail</servlet-name>
    <servlet-class>Servlet.LeaveDetailServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LeaveDetail</servlet-name>
    <url-pattern>/LeaveDetail</url-pattern>
  </servlet-mapping>
   
  <servlet>
    <servlet-name>LeaveType</servlet-name>
    <servlet-class>Servlet.LeaveTypeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LeaveType</servlet-name>
    <url-pattern>/LeaveType</url-pattern>
  </servlet-mapping>
  
  
   <servlet>
    <servlet-name>Accept</servlet-name>
    <servlet-class>Servlet.AcceptRejectServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Accept</servlet-name>
    <url-pattern>/acceptReject</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>status</servlet-name>
    <servlet-class>Servlet.LeaveStatusServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>status</servlet-name>
    <url-pattern>/leaveStatus</url-pattern>
  </servlet-mapping>
  
  
</web-app>